                                                                <i class="fa fa-check-square"></i>Children’s play area
                                                            </li>
                                                            <% } if(Amenities[0].Club_House) {%>
                                                            <li class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                                                <i class="fa fa-check-square"></i>Club house
                                                            </li>
                                                            <% } if(Amenities[0].Multipurpose_Room) {%>
                                                            <li class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                                                <i class="fa fa-check-square"></i>Multipurpose room
                                                            </li>
                                                            <% } if(Amenities[0].Sports_Facility) {%>
                                                            <li class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
                                                                <i class="fa fa-check-square"></i>Sports facility
                                                            </li>
