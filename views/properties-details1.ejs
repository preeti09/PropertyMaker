                                        <li><a href="#Overview">Overview</a></li>
                                        <% } if(propertyResult[0].VideoUrl || propertyResult[0].VideoGallery){ %>
                                        <li><a href="#Video">Video</a></li>
                                        <% } if(Floor_Plans.length > 0){ %>
                                        <li><a href="#FloorPlan">Floor Plan</a></li>
                                        <% } if(Amenities.length > 0){ %>
                                        <li><a href="#Amenities">Amenities</a></li>
                                        <% } if(Specifications.length > 0){ %>
                                        <li><a href="#Specifications">Specifications</a></li>
                                        <li><a href="#Specifications">Price Trends</a></li>
                                        <% } %>    
                                        <li><a href="#neighbourhood">Location</a></li>
                                        <% if(Apartments_Projects_Houses.length > 0 && Apartments_Projects_Houses[0].Brochure_Url){%>
                                        <li><a href="#Brochure">Brochure</a></li>
                                        <% } %>
                                    </ul>
                                </div>
                            </nav>
                            <div class="with-nav-tabs">
                                <div class="">
                                    <% if(Apartments_Projects_Houses.length > 0){%>
                                    <div id="Overview" class="mb-50">
                                        <div class="main-title-2">
                                            <h1 class="clrGreen"><span>Overview</span></h1>
                                        </div>
                                        <div class="row mb-30">
                                            <div class="col-md-4 col-sm-6 mb-30">
                                                <div class="project-spec">
                                                    <h4><span><%= propertyResult[0].Facing %></span></h4>
                                                    <span class="gray-clr">Facing of all the Flats</span>
                                                </div>
                                            </div>
                                            <div class="col-md-4 col-sm-6 mb-30">
                                                <div class="project-spec">
                                                    <h4><span><%= Apartments_Projects_Houses[0].Site_Area %></span></h4>
                                                    <span class="gray-clr">Site Area</span>
                                                </div>
                                            </div>
                                            <div class="col-md-4 col-sm-6 mb-30">
                                                <div class="project-spec">
                                                    <h4><span><%= Apartments_Projects_Houses[0].Total_Units %></span></h4>
                                                    <span class="gray-clr">Total Units</span>
                                                </div>
                                            </div>

                                            <div class="col-md-4 col-sm-6 mb-30">
                                                <div class="project-spec">
                                                    <h4><span><%= Apartments_Projects_Houses[0].Total_Floors %></span></h4>
                                                    <span class="gray-clr">Total Floors</span>
                                                </div>
                                            </div>
                                            <div class="col-md-4 col-sm-6 mb-30">
                                                <div class="project-spec">
                                                    <h4><span><%= Apartments_Projects_Houses[0].Total_Towers %></span></h4>
                                                    <span class="gray-clr">Total Towers</span>
                                                </div>
                                            </div>
                                            <div class="col-md-4 col-sm-6 mb-30">
