                    <h4>PROPERTIES SIMILAR TO <b><%= propertyDetails.Title %></b></h4>
                </div>
                <div class="row">
                    
                    <% for(var i = 0; i < similarProperties.length; i++){
                       var ImageGallery= similarPropertiesImageGallery;
                       var TotalPrice = [];
                       var Extent = [];
                       var unitDetailsData = SimilarUnitData[similarProperties[i].ID];
                       var UnitData = [];
                       
                       if(typeof SimilarUnitData[similarProperties[i].ID] != 'undefined' &&typeof SimilarUnitData[similarProperties[i].ID] == 'object' && SimilarUnitData[similarProperties[i].ID].length){
                           for(var u = 0; u < SimilarUnitData[similarProperties[i].ID].length; u++){
                               if(similarProperties[i].ID == SimilarUnitData[similarProperties[i].ID][u].PropertyID ){
                                   UnitData.push(SimilarUnitData[similarProperties[i].ID][u]);
                                   if(SimilarUnitData[similarProperties[i].ID][u].UnitExtent){
                                       TotalPrice.push(parseInt(SimilarUnitData[similarProperties[i].ID][u].UnitExtent) * parseInt(similarProperties[i].SearchPrice));
                                       Extent.push(parseInt(SimilarUnitData[similarProperties[i].ID][u].UnitExtent));
                                   }else
                                   if(SimilarUnitData[similarProperties[i].ID][u].SuperBuiltUpArea){
                                       TotalPrice.push(parseInt(SimilarUnitData[similarProperties[i].ID][u].SuperBuiltUpArea) * parseInt(similarProperties[i].SearchPrice));  
                                       Extent.push(parseInt(SimilarUnitData[similarProperties[i].ID][u].SuperBuiltUpArea));
                                   }
                                   else if(similarProperties[i].Extent || similarProperties[i].TotalPrice){
                                       if(similarProperties[i].TotalPrice){
                                       TotalPrice.push(parseInt(similarProperties[i].TotalPrice)); 
                                       }
                                       if(similarProperties[i].Extent){
                                       Extent.push(parseInt(similarProperties[i].Extent));  
                                       }
                                   }
                               }
                           }
                       }else{
                           TotalPrice.push(parseInt(similarProperties[i].TotalPrice));
                           Extent.push(parseInt(similarProperties[i].Extent));
                       }
                       similarProperties[i].unitDetails= UnitData;
                    %>
               
                    <div class="col-lg-3 col-md-6 col-sm-6 col-xs-12 grid-opt">
                        <div class="property">
                            <!-- Property img -->
                            <div class="property-img">
                                    <% 
                                    var Active = '';
                                    if(favoriteData.length > 0){
                                    if(favoriteData.includes(similarProperties[i].ID))
                                    Active = 'active'; 
                                    }
                                    %>
                                    <div class="property-tag alt featured <%= Active %>" id="myfavorite<%= i %>">
                                        <a href="javascript:void(0);" onclick="saveAsFavorite('<%= sess_val %>',<%= similarProperties[i].ID %>,<%= i %>);"><i class="fa fa-heart" aria-hidden="true"></i></a>
                                    </div>
                                <!-- <div class="property-tag sale">TRENDING</div> -->
                                <div class="property-price"><%= similarProperties[i].Title %>
                                    <% if(similarProperties[i].DisplayPrice) { %>   
                                    <% if(Math.min(...TotalPrice) == Math.max(...TotalPrice)){ 
                                        similarProperties[i].viewPrice = currencyFormat(Math.max(...TotalPrice));    
                                    %>
                                    <div>&#8377; <%= currencyFormat(Math.min(...TotalPrice)) %></div>
                                    <% }else{ 
                                        similarProperties[i].viewPrice = currencyFormat(Math.min(...TotalPrice))+ ' - ' + currencyFormat(Math.max(...TotalPrice));
                                    %>
                                        <div>&#8377; <%= currencyFormat(Math.min(...TotalPrice)) %> - <%= currencyFormat(Math.max(...TotalPrice)) %></div>    
                                    <% } } else { %>
                                    <div> Price on Request </div>
                                    <% } %>
                                </div>
                                <!-- Facilities List -->
                                <ul class="facilities-list clearfix">
                                    <li>
                                        <% if(Extent && Extent.length > 0){ %>
                                            <img src="/img/extent.jpg" width="14" ,height="16" style="margin-top:-4px" title="Extent">
                                            <span>    
                                            <% if(Math.min(...Extent) == Math.max(...Extent)){ %>
                                            <%= Math.min(...Extent) %> <%= propertyDetails.UnitIdentifier %>
                                            <% }else{ %>        
                                            <%= Math.min(...Extent) %> - <%= Math.max(...Extent) %> <%= propertyDetails.UnitIdentifier %>   
                                            <% } %>
                                            </span>
                                            <% } %>
                                    </li>
                                </ul>
                                <% if(ImageGallery[similarProperties[i].ID]) { %>
                                <img src="<%= ImageGallery[similarProperties[i].ID][0] %>" alt="fp" class="img-responsive">
                                <% } else{ %>
                                <img src="<% if(propertyDetails.PropertyTypeID == 1){%>/img/Plots.png<% } else if(propertyDetails.PropertyTypeID == 2){%>/img/Residential.png<% }else if(propertyDetails.PropertyTypeID == 3){%>/img/AgriLands.png<%}%>" alt="fp" class="img-responsive">
                                <% } %>
                                <div class="property-overlay">
                                    <a href="<%= similarProperties[i].URL %>" class="overlay-link">
                                        <i class="fa fa-link"></i>
                                    </a>
                                    <!-- <a class="overlay-link property-video" title="Lexus GS F">
                                        <i class="fa fa-video-camera"></i>
                                    </a> -->
                                    <div class="property-magnify-gallery">
                                        <% if(ImageGallery[similarProperties[i].ID]) { %>
                                        <% for(var j= 0; j < ImageGallery[similarProperties[i].ID].length; j++){%>
                                        <% if(j == 0){ %>
                                        <a href="<%= ImageGallery[similarProperties[i].ID][j] %>" class="overlay-link">
                                            <i class="fa fa-expand"></i>
                                        </a>
                                        <% }else{ %>
                                        <a href="<%= ImageGallery[similarProperties[i].ID][j] %>" class="hidden"></a>
                                        <% } 
                                        } %>
                                        <% } %>
                                    </div>
                                </div>
                            </div>
                            <!-- Property content -->
                            
                        </div>
                    </div>
                    <% } %>
                    <% if(similarProperties.length == 0){ %>
                        <div class="col-xs-12">No Similar Property Found!</div>
                    <% } %>
                </div>
            </div>
            <div class="row pd-70">
                <div class="col-xs-12">
                    <div class="form-group submit-button text-center">
                      <!--   <a href="javascript:void(0);" class="button" onclick='iAm_Interested(<%= JSON.stringify(propertyDetails) %>'> I am Interested </a> -->
                      <a href="javascript:void(0);" class="button iam" onclick='iAm_Interested(<%= JSON.stringify(propertyDetails)%>,<%= JSON.stringify(PropertyType[0]) %>)' > I am Interested </a>
                  </div>
                </div>
            </div>
        </div>
        </div>
    </div>
    <!-- Featured properties end -->
</section>
<% }else{ %>
    <section class="content-area submit-property-detail">
        <br><br>
        <center><h2>Oops, sorry! no data available for this property.</h2></center>
        <br><br><br>
    </section>
<% } %>

