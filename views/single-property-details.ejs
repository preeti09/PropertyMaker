            <% }if(propertyDetails && propertyDetails.Area){%>    
            <li><a href="/buy/search?type=<%=propertyDetails.Type%>&area=<%= propertyDetails.Area %>&city=<%=propertyDetails.city %>" target="_blank"><%= propertyDetails.Area %></a> / </li>
            <% }if(propertyDetails && propertyDetails.Title){%>
