            <% }%>
    </ul>
</div>

<% if(propertyDetails && propertyDetails.ID ){ 
    var min;
    var max;
    if(priceCalculation && priceCalculation[propertyDetails.ID]){
     min = Math.min(...priceCalculation[propertyDetails.ID])*parseInt(propertyDetails.SearchPrice);
    
     max = Math.max(...priceCalculation[propertyDetails.ID])*parseInt(propertyDetails.SearchPrice);
    }
    if(min && max && min == max){
        propertyDetails.viewPrice = currencyFormat(min);
    }else if(min && max){
        propertyDetails.viewPrice = currencyFormat(min)+' - '+currencyFormat(max);
    }else{
        propertyDetails.viewPrice = currencyFormat(propertyDetails.TotalPrice); 
    }
    if(UnitData){
        propertyDetails.unitDetails = UnitData[propertyDetails.ID] ? UnitData[propertyDetails.ID] : [];
    }
%>
