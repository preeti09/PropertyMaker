            format: 'mm/dd/yyyy',
            container: container,
            todayHighlight: true,
            autoclose: true
        })
        var d = new Date();
        $('input[name="createDate"]').val((d.getMonth() + 01) + '/' + d.getDate() + '/' + d.getFullYear());

        // Add Class for Shortlist
        $('.short-list').click(function(event) {
            event.preventDefault();
            $(this).toggleClass('active');
            var status = 0;
            if ($(this).hasClass('active')) {
                status = 1;
            }
            var data = {
                status: status,
                Propid: $(this).attr('Propid')
            };
            saveAsFavourite(data);
        })

        // Tooltips
        
        $('[data-toggle="tooltip"]').tooltip();
        

        // Smooth Scroll
        
        $('body').scrollspy({
            target: ".navbar"
        });

        $("#Navbar a").on('click', function(event) {
            if (this.hash !== "") {
                event.preventDefault();
                var hash = this.hash;
                $('html, body').animate({
                    scrollTop: $(hash).offset().top - 120
                }, 800, );
            }
        });
