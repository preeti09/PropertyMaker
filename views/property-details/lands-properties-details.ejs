<%- include('../header') -%>
<%- include('../banner') -%>

<!-- Properties details page start -->
<div class="properties-details-page mb-50">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                <div class="breadcrumb-area bot-breadcrumb">
                    <ul class="breadcrumbs">
                        <li><a href="index.html">Home</a></li>
                        <li class="active">Plot Detail</li>
                    </ul>
                </div>
                

                <!-- Properties detail slider start -->
                <div class="properties-detail-slider simple-slider mb-40">
                    <div id="carousel-custom" class="carousel slide" data-ride="carousel">
                        <div class="carousel-outer">
                            <!-- Wrapper for slides -->
                            <div class="carousel-inner">
                                <div class="item">
                                    <img src="http://placehold.it/1140x500" class="thumb-preview" alt="Chevrolet Impala">
                                </div>
                                <div class="item">
                                    <img src="http://placehold.it/1140x500" class="thumb-preview" alt="Chevrolet Impala">
                                </div>
                                <div class="item">
                                    <img src="http://placehold.it/1140x500" class="thumb-preview" alt="Chevrolet Impala">
                                </div>
                                <div class="item">
                                    <img src="http://placehold.it/1140x500" class="thumb-preview" alt="Chevrolet Impala">
                                </div>
                                <div class="item">
                                    <img src="http://placehold.it/1140x500" class="thumb-preview" alt="Chevrolet Impala">
                                </div>
                                <div class="item">
                                    <img src="http://placehold.it/1140x500" class="thumb-preview" alt="Chevrolet Impala">
                                </div>
                                <div class="item">
                                    <img src="http://placehold.it/1140x500" class="thumb-preview" alt="Chevrolet Impala">
                                </div>
                                <div class="item active">
                                    <img src="http://placehold.it/1140x500" class="thumb-preview" alt="Chevrolet Impala">
                                </div>
                            </div>
                            <!-- Controls -->
                            <a class="left carousel-control" href="#carousel-custom" role="button" data-slide="prev">
                                    <span class="slider-mover-left no-bg t-slider-r pojison" aria-hidden="true">
                                        <i class="fa fa-angle-left"></i>
                                    </span>
                                <span class="sr-only">Previous</span>
                            </a>
                            <a class="right carousel-control" href="#carousel-custom" role="button" data-slide="next">
                                    <span class="slider-mover-right no-bg t-slider-l pojison" aria-hidden="true">
                                        <i class="fa fa-angle-right"></i>
                                    </span>
                                <span class="sr-only">Next</span>
                            </a>
                        </div>
                        <!-- Indicators -->
                        <ol class="carousel-indicators thumbs visible-lg visible-md">
                            <li data-target="#carousel-custom" data-slide-to="0" class=""><img src="http://placehold.it/90x60" alt="Chevrolet Impala"></li>
                            <li data-target="#carousel-custom" data-slide-to="1" class=""><img src="http://placehold.it/90x60" alt="Chevrolet Impala"></li>
                            <li data-target="#carousel-custom" data-slide-to="2" class=""><img src="http://placehold.it/90x60" alt="Chevrolet Impala"></li>
                            <li data-target="#carousel-custom" data-slide-to="3" class=""><img src="http://placehold.it/90x60" alt="Chevrolet Impala"></li>
                            <li data-target="#carousel-custom" data-slide-to="4" class=""><img src="http://placehold.it/90x60" alt="Chevrolet Impala"></li>
                            <li data-target="#carousel-custom" data-slide-to="5" class=""><img src="http://placehold.it/90x60" alt="Chevrolet Impala"></li>
                            <li data-target="#carousel-custom" data-slide-to="6" class=""><img src="http://placehold.it/90x60" alt="Chevrolet Impala"></li>
                            <li data-target="#carousel-custom" data-slide-to="7" class=""><img src="http://placehold.it/90x60" alt="Chevrolet Impala"></li>
                        </ol>
                    </div>
                </div>
                <!-- Properties detail slider end -->

            </div>
        </div>
        <div class="row">
            <div class="col-lg-8 col-md-8 col-sm-12 col-xs-12">
                <!-- Properties details section start -->
                <div class="properties-details-section sidebar-widget">
                    
                    <div class="main-title-2">
                        <div class="property-name">
                            <h1 class="mar-b-0"><span>Property Name</span></h1>
                            <div class="title-builder">
                                <span>Extent </span><a href="#" class="more-link no-ajaxy">Category Name</a>
                            </div>
                        </div>
                        <div class="short-list">
                            <i class="fa fa-heart"></i>
                        </div>
                    </div>

                    <div class="max-width project-section">
                        <div class="location-wrap mb-30">
                            <h4>   
                                <i class="fa fa-map-marker clrGreen" aria-hidden="true"></i>&nbsp;
                                <span class="va-middle proj-address">
                                    <a href="/mumbai/property-sale-dombivali-50030" target="_blank" class="reset-a no-ajaxy">Area, City</a>
                                    <span class="js-header-tabs">
                                        <a href="#neighbourhood" data-tracking="viewOnMap" class="more-link more-link map-link">&nbsp;(show on map)</a>
                                    </span>
                                </span>
                            </h4>
                        </div>
                        <div class="row mb-30">
                            
                            <div class="col-sm-4">
                                <div class="project-spec">
                                    <h4><span>$ 32.80 L - $ 60.10 L</span></h4>
                                    <span class="gray-clr">Builder price</span>
                                </div>
                            </div>
                            
                            <div class="col-sm-4">
                                <div class="project-spec">
                                    <h4><span>Title of the property</span></h4>
                                    <h5><span class="extent">extent </span><span class="category">category </span><span>plot for sale in </span><span class="area">area, </span><span class="city">City</span></h5>
                                </div>
                            </div>

                        </div>

                    </div>
                    <ul class="social-list clearfix share-post mb-30">
                        <li><h4>Share with</h4></li>
                        <li>
                            <a href="#" class="facebook">
                                <i class="fa fa-facebook"></i>
                            </a>
                        </li>
                        <li>
                            <a href="#" class="twitter">
                                <i class="fa fa-twitter"></i>
                            </a>
                        </li>
                        <li>
                            <a href="#" class="google">
                                <i class="fa fa-google-plus"></i>
                            </a>
                        </li>
                    </ul>
                    <!-- Property description start -->
                    <div class="Property-description">
                        <nav class="navbar nav-smooth_scroll mb-50 row">
                            <div id="Navbar">
                              <ul class="nav navbar-nav">
                                <li><a href="#Overview">Overview</a></li>
                                <li><a href="#nearby">Nearby</a></li>
                                <li><a href="#priceTrends">Price Trends</a></li>
                                <li><a href="#Location">Location</a></li>
                              </ul>
                            </div>
                        </nav>  
                        <div class="with-nav-tabs">
                            <div class="">
                                
                                <div id="Overview" class="mb-50">
                                    <div class="main-title-2">
                                      <h1 class="clrGreen"><span>Overview</span></h1>
                                    </div>
                                    <div class="row mb-30">
                                        <div class="col-md-4 col-sm-6 mb-30">
                                            <div class="project-spec">
                                                <h4><span><%= Plots_Lands[0].Facing %></span></h4>
                                                <span class="gray-clr">Facing</span>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-sm-6 mb-30">
                                            <div class="project-spec">
                                                <h4><span><%= Plots_Lands[0].AbRoadWidth %></span></h4>
                                                <span class="gray-clr">Abutting Road Width</span>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-sm-6 mb-30">
                                            <div class="project-spec">
                                                <h4><span><%= Plots_Lands[0].PropertyWidth %></span></h4>
                                                <span class="gray-clr">Width</span>
                                            </div>
                                        </div>

                                        <div class="col-md-4 col-sm-6 mb-30">
                                            <div class="project-spec">
                                                <h4><span><%= Plots_Lands[0].PropertyDepth %></span></h4>
                                                <span class="gray-clr">Depth</span>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-sm-6 mb-30">
                                            <div class="project-spec">
                                                <h4><span><%= Plots_Lands[0].Fenced %></span></h4>
                                                <span class="gray-clr">Fenced</span>
                                            </div>
                                        </div>

                                        <div class="col-md-4 col-sm-6 mb-30">
                                            <div class="project-spec">
                                                <h4><span><%= Plots_Lands[0].RecentCrop %></span></h4>
                                                <span class="gray-clr">Recent Crop Cultivated</span>
                                            </div>
                                        </div>

                                        <div class="col-md-4 col-sm-6 mb-30">
                                            <div class="project-spec">
                                                <h4><span><%= Plots_Lands[0].Soil %></span></h4>
                                                <span class="gray-clr">Soil</span>
                                            </div>
                                        </div>

                                        <div class="col-md-4 col-sm-6 mb-30">
                                            <div class="project-spec">
                                                <h4><span><%= Plots_Lands[0].PlantationAge %></span></h4>
                                                <span class="gray-clr">Plantation Age</span>
                                            </div>
                                        </div>

                                        <div class="col-md-4 col-sm-6 mb-30">
                                            <div class="project-spec">
                                                <h4><span><%= Plots_Lands[0].WaterSource %></span></h4>
                                                <span class="gray-clr">Water Source</span>
                                            </div>
                                        </div>

                                        <div class="col-md-4 col-sm-6 mb-30">
                                            <div class="project-spec">
                                                <h4><span><%= Plots_Lands[0].NoBores %></span></h4>
                                                <span class="gray-clr">No of Bores</span>
                                            </div>
                                        </div>
                                        
                                        <div class="clearfix"></div>
                                        <div class="col-md-12">
                                            <div class="">
                                                <h4><strong>Description of the Property</strong></h4>
                                                <p class="gray-clr"><%= Plots_Lands[0].PropertyDesc %></p>
                                            </div>
                                        </div>
                                    
                                    </div>
                                </div>

                                <div id="nearby" class="">
                                      <!-- Inside properties start  -->
                                      <div class="inside-properties">
                                          <!-- Main Title 2 -->
                                          <div class="main-title-2">
                                              <h1 class="clrGreen"><span>Nearby </span></h1>
                                          </div>
                                          <div class="row">
                                              <div class="col-md-12 mb-50">
                                                    <p class="gray-clr"><%= propertyResult[0].Nearby %></p>
                                                </div>
                                        </div>
                                      </div>
                                </div>
                                <div class="clearfix"></div>
                                <% if(Floor_Plans.length > 0){ %>
                                <div id="priceTrends" class="mb-50">
                                  <div class="">
                                    <!-- Properties condition start -->
                                    <div class="properties-condition">
                                        <div class="main-title-2">
                                              <h1 class="clrGreen"><span>Price Trands </span></h1>
                                          </div>
                                        <% for(var i =0; i < Floor_Plans.length; i++){ %>
                                        <div class="row mb-30">
                                            <div class="col-md-4 col-sm-6 mb-30">
                                                <div class="project-spec">
                                                    <h4><span><%= Floor_Plans[i].Title %></span></h4>
                                                    <span class="gray-clr">Title</span>
                                                </div>
                                            </div>
                                            <div class="col-md-4 col-sm-6 mb-30">
                                                <div class="project-spec">
                                                    <h4><span><%= Floor_Plans[i].Bed_No %></span></h4>
                                                    <span class="gray-clr">No of Bed rooms</span>
                                                </div>
                                            </div>
                                            <div class="col-md-4 col-sm-6 mb-30">
                                                <div class="project-spec">
                                                    <h4><span><%= Floor_Plans[i].Bath_No %></span></h4>
                                                    <span class="gray-clr">No of Bath Rooms</span>
                                                </div>
                                            </div>
                                            <div class="col-md-4 col-sm-6 mb-30">
                                                <div class="project-spec">
                                                    <h4><span><%= Floor_Plans[i].Super_Built_Area %></span></h4>
                                                    <span class="gray-clr">Super Built up area</span>
                                                </div>
                                            </div>
                                            <div class="col-md-4 col-sm-6 mb-30">
                                                <div class="project-spec">
                                                    <h4><span><%= Floor_Plans[i]['Plinth Area'] %></span></h4>
                                                    <span class="gray-clr">Carpet Area</span>
                                                </div>
                                            </div>
                                            <div class="col-md-4 col-sm-6 mb-30">
                                                <div class="project-spec">
                                                    <h4><span><%= Floor_Plans[i].Car_Park %></span></h4>
                                                    <span class="gray-clr">Car parking</span>
                                                </div>
                                            </div>
                                            <div class="col-md-4 col-sm-6 mb-30">
                                                <div class="project-spec">
                                                    <h4><span><%= Floor_Plans[i].Balcony %></span></h4>
                                                    <span class="gray-clr">Balconies</span>
                                                </div>
                                            </div>
                                            <div class="col-md-4 col-sm-6 mb-30">
                                                <div class="project-spec">
                                                    <h4><span><%= Floor_Plans[i].Undivided_Share %></span></h4>
                                                    <span class="gray-clr">Undivided Share</span>
                                                </div>
                                            </div>
                                            <div class="col-md-4 col-sm-6 mb-30">
                                                <div class="project-spec">
                                                    <h4><span><%= Floor_Plans[i].Facing %></span></h4>
                                                    <span class="gray-clr">Facing</span>
                                                </div>
                                            </div>
                                            <div class="clearfix"></div>
                                        </div>
                                        <% } %>
                                    </div>
                                    <!-- Properties condition end -->
                                  </div>
                                </div>
                                <% } %>
                                <div id="Location" class="mb-30">
                                    <!-- Properties amenities start -->
                                    <div class="properties-amenities">
                                        <div class="main-title-2">
                                              <h1 class="clrGreen"><span>Location </span></h1>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm-12">
                                                <div id="map" class="contact-map"></div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                
                            </div>
                        </div>
                    </div>
                    <!-- Property description end -->
                </div>
                <!-- Properties details section end -->
                <div class="clearfix"></div>
               
            </div>
            <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                <!-- Sidebar start -->
                <div class="sidebar right">

                    <!-- Search contents sidebar start -->
                    <div class="main-title-2 contactUs">
                        <p>Interested to buy property in</p>
                        <h1><span>Builder Name</span></h1>
                    </div>
                    <div class="sidebar-widget hidden-sm hidden-xs">

                        <form method="GET">
                            <div class="form-group">
                                <label for="name">Name</label>
                                <input type="text" id="name" placeholder="Name" class="form-control" name="">
                            </div>
                            <div class="form-group">
                                <label for="email">Email</label>
                                <input type="email" id="email" class="form-control" placeholder="Email" name="">
                            </div>
                            <div class="form-group">
                                <label for="phone">Mobile Number</label>
                                <input id="phone" class="form-control" placeholder="xxx xxx xxxx" type="tel">
                            </div>

                            <div class="form-group mb-0">
                                <button class="search-button"><i class="fa fa-phone" aria-hidden="true"></i> Get Call Back</button>
                            </div>
                        </form>
                    </div>
                    <!-- Search contents sidebar end -->

                    <!-- Social media start -->
                    <div class="social-media sidebar-widget clearfix">
                        <!-- Main Title 2 -->
                        <div class="main-title-2">
                            <h1><span>Social</span> Media</h1>
                        </div>
                        <!-- Social list -->
                        <ul class="social-list">
                            <li><a href="#" class="facebook-bg"><i class="fa fa-facebook"></i></a></li>
                            <li><a href="#" class="twitter-bg"><i class="fa fa-twitter"></i></a></li>
                            <li><a href="#" class="linkedin-bg"><i class="fa fa-linkedin"></i></a></li>
                            <li><a href="#" class="google-bg"><i class="fa fa-google-plus"></i></a></li>
                            <li><a href="#" class="rss-bg"><i class="fa fa-rss"></i></a></li>
                        </ul>
                    </div>
                    <!-- Social media end -->

                    <div class="clearfix"></div>

                </div>
                <!-- Sidebar end -->
            </div>
        </div>



    </div>
</div>
<!-- Properties details page end -->

<!-- Property CTA -->

<section id="FooterCTA">
    <div class="container">
        <div class="row">
            <div class="col-sm-12 text-center">
            <div class="footerForm">
            <div class="main-title-2">
                <h1><span class="clrGreen">Have any Questions?</span></h1>
                <p>Connect with our property Expert!</p>
            </div>
                <form method="GET">
                    <div class="col-sm-4">
                        <div class="form-group text-left">
                            <label for="name">Name</label>
                            <input type="text" id="name" placeholder="Name" class="form-control" name="">
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group text-left">
                            <label for="email">Email</label>
                            <input type="email" id="email" class="form-control" placeholder="Email" name="">
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group text-left">
                            <label for="phone">Mobile Number</label>
                            <input id="mobile" class="form-control" placeholder="xxx xxx xxxx" type="tel">
                        </div>
                    </div>
                    <div class="col-sm-12">
                        <div class="form-group mb-0">
                            <button class="search-button"><i class="fa fa-phone" aria-hidden="true"></i> Get Call Back</button>
                        </div>
                    </div>
                </form>
            </div>
            </div>
        </div>
    </div>
</section>
<!-- END -->

